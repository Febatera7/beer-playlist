{
  "name": "beer-playlist",
  "version": "1.0.0",
  "description": "An app that chooses the ideal playlist and beer based on the temperature you want your beer to be.",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "build": "tsc -p tsconfig.json",
    "dev": "tsnd --cls --rs --exit-child --transpile-only -- src/server.ts",
    "format": "prettier --write \"src/**/*.ts\" && yarn lint",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "test": "echo \"Error: no test specified\" && exit 1",
    "typeorm": "ts-node-dev ./node_modules/typeorm/cli.js"
  },
  "author": "Felipe Nunes",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.3.5",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "memory-cache": "^0.2.0",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^5.1.6",
    "typeorm": "^0.3.12",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/memory-cache": "^0.2.2",
    "@types/node": "^18.15.11",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.7",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.3"
  }
}
